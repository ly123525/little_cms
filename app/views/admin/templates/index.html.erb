<div class="page_header">
  <div class="right_buts pull-right">
    <%= link_to '新增模板', new_admin_template_path, :class=>'btn btn-success' %>
  </div>
  <h4>模板列表</h4>
</div>
<table class="table table-striped table-bordered table-hover">
  <% if @templates.any? %>
      <col width="100"/>
      <col/>
      <col width="200"/>
      <col width="100"/>
      <col width="200"/>
      <col width="150"/>
      <thead>
      <tr>
        <td>编号</td>
        <td>名称</td>
        <td>文件名</td>
        <td>渲染类型</td>
        <td>创建时间</td>
        <td>操作</td>
      </tr>
      </thead>
      <tbody id="templates">
      <%= render @templates %>
      </tbody>
  <% else %>
      <%= render "admin/base/table_no_data" %>
  <% end %>
</table>
<div class="text-center">
  <%= will_paginate @templates %>
</div>